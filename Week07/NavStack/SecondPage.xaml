<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Week7NavStack.SecondPage"
             Title="Second Page">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to the second page"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Label x:Name="namelbl" HorizontalOptions="Center"/>
        <Entry x:Name="SendEntry"/>

        <Button Text="Go Back" Clicked="OnBackButtonClicked" />
        <Button Text="Go Back Send Data" Clicked="OnBackButtonSendClicked"/>
    </VerticalStackLayout>
</ContentPage>